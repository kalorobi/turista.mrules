//út típusok
	target : highway*
//fout
		if : *primary*
			define
				min-zoom : 10
				line-color : #FFF509
				line-width : 10:2;12:2;15:7;18:8
				border-style : solid
				border-color : #CD6506 black 20%
				border-width : 15%
			draw : line
			define
				text-align-horizontal : center
				text-align-vertical : center
				font-style : normal
				font-weight : bold
				font-size : 12
				min-zoom : 12
				text : @if(ref, ref)
				text-color : white
				text-max-width : 15
				shape : custom
				shape-def : p:white;pw:0.5;f:green;m:-40,24;l:40,24;l:40,-30;l:0,-50;l:-40,-30;z
				shield-padding-left : 8
				shield-padding-right : 8
				shield-padding-top : 5
				shield-padding-bottom : 8
				shield-resize-mode : free
			draw : shield
//masodrendu ut
		elseif : *secondary
			define
				min-zoom : 10
				line-color : #FFFFFF
				line-width : 10:1.5;13:1.8;15:5;18:6
				border-style : solid
				border-color : #D5D5D5 black 20%
				border-width : 15%
			draw : line			
		elseif : *tertiary
			define
				min-zoom : 10
				line-color : #FFFFFF
				line-width : 10:1.5;13:1.8;15:5;18:6
				border-style : solid
				border-color : #D5D5D5 black 20%
				border-width : 15%
			draw : line
//szervizut
		elseif : *service
			define
				min-zoom : 13
				line-color : white
				line-width : 12:0.5;13:1;15:3;18:5
				border-style : solid
				border-color : #D5D5D5 black 20%
				border-width : 15%
			draw : line
//foldut
		elseif : *track
			define
				min-zoom : 12.5
				line-color : #7B7B7B
				line-width : 12.5:0.8;13:1;15:1.2;18:1.8
				line-style : dash
			for : tracktype=grade1
				define
					line-style : solid
					line-width : 12.5:0.8;13:1.2;15:1.8;18:2.2
					border-style : solid
					border-color : white
					border-width : 100%
					border-opacity : 0.3
			for : tracktype=grade2
				define
					line-style: dashlong
					border-style : solid
					border-color : white
					border-width : 100%
					border-opacity : 0.3
			for : tracktype=grade3
				define
					line-style: dash
			for : tracktype=grade4
				define
					line-style: dashdot
			for : tracktype=grade5
				define
					line-style: dashdotdot
			for : tunnel=yes
				define
					border-style : dot
			for : bridge=yes
				define
					border-style : solid
					border-color : white
					border-width : 100%
					border-opacity : 0.3
					border-color : #000
			draw : line
//utcák felírattal
		elseif : *residential
			define
				min-zoom : 13
				line-color : white
				line-width : 12:0.5;13:1;15:3;18:5
				border-style : solid
				border-color : #D5D5D5 black 20%
				border-width : 15%
			draw : line
			define
				min-zoom : 15
				max-zoom : 20
			draw : text

//osveny
		elseif : *path
			define
				min-zoom : 12.5
				line-color : #7B7B7B
				line-width : 12.5:0.8;13:1;15:1.2;18:1.8
				line-style : dashdotdot
			draw : line
