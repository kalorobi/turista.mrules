		gps tracks : gpstrack[]
		gps routes : gpsroute[]

	points
		place city : place=city
		place town : place=town
		place village : place=village
		place hamlet : place=hamlet
		place locality: place=locality
		place suburb : place=suburb
		railway station : railway=station
		highway_bus_stop : highway=bus_stop
		amenity bus_station : amenity=bus_station
		spring : natural=spring
		peak : natural=peak
		cross : man_mde=cross OR historic=wayside_cross
		viewpoint : tourism=viewpoint
		cave : natural=cave_entrance
		cliff : natural=cliff
		gate : barrier=gate OR barrier=lift_gate
		power_pole : power=pole OR power=tower
		tower : tower:type=observation
		hunting : amenity=hunting_stand
		picnic : tourism=picnic_site OR leisure=picnic_table
		memorial : historic=memorial
		tree : natural=tree
		
properties
	map-background-color	: #F1EEE8
	map-background-opacity	: 1
	map-sea-color : #B5D0D0
	font-weight : bold
	font-family : Special Elite
	text-max-width : 7
	text-halo-width : 0%
	text-halo-opacity : 1
	text-align-horizontal : center
	text-align-vertical : center
	font-stretch : 0.9
	icon-width : 18
	map.rendering.lflp.min-buffer-space : 5
	map.rendering.lflp.max-allowed-corner-angle : 40

rules
//gps
	target: gps tracks
		define
			min-zoom : 13
			line-color : #993300
			line-opacity : 0.75
			line-width : 2
		draw : line

	target: gps routes
		define
			line-color : blue
			line-opacity : 0.75
			line-style : dash
			min-zoom : 5
			line-width : 5
		draw : line
//pontok
	target : $featuretype(point)
		if : place*
			define
				font-weight : bold
				text-max-width : 1000
				text-align-horizontal : near
//település nevek
			if : *city
				define
					font-size : 6:12;14:24
					min-zoom : 6
					max-zoom : 14
					placement-value : 1000
			elseif : *town
				define
					font-size : 6:10;12:12;20:22
					min-zoom : 6
					placement-value : 20
			elseif : *village
				define
					font-size : 9:8;20:20
					min-zoom : 9
					placement-value : 1
			elseif : *hamlet
				define
					font-size : 10:8;20:16
					min-zoom : 10
					placement-value : 0.1
			elseif : *locality
				define
					font-size : 14:8;20:16
					min-zoom : 14
					placement-value : 0.1
			draw : text
//feszulet
	target : cross
		define
			min-zoom : 13
			icon-image : icons/jelkulcs/feszulet.png
		draw : icon
//templom
	target : church
		define
			min-zoom : 13
			icon-image : icons/jelkulcs/templom.png
		draw : icon
		define
			font-style : italic
			font-weight : normal
			font-size : 13:6;15:10
			min-zoom : 13
			text : @if(name, name, "templom")
			text-align-horizontal : near
			text-color : #C00A81
			text-max-width : 24
			text-offset-vertical : 80%
		draw : text
//kilatopont
	target : viewpoint
		define
			min-zoom : 13
			icon-width : 12
			icon-image : icons/jelkulcs/kilatas.png
		draw : icon
//kilato
